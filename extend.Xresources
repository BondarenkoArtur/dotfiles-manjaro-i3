! Themes
*background:            #1f2126
*foreground:            #5F6B85
*fading:                15
*fadeColor:             black
*cursorColor:           #5F6B85
*pointerColorBackground:#2B2C2B
*pointerColorForeground:#16A085

!! black dark/light
*color0:                #000000
*color8:                #1f2126

!! red dark/light
*color1:                #E06C75
*color9:                #E06C75

!! green dark/light
*color2:                #98C379
*color10:               #98C379

!! yellow dark/light
*color3:                #E5C07B
*color11:               #E5C07B

!! blue dark/light
*color4:                #56B6C2
*color12:               #56B6C2

!! cyan dark/light
*color6:                #61AEEE
*color14:               #61AEEE

!! white dark/light
*color7:                #5F6B85
*color15:               #FFFFFF

! Xcursor
Xcursor.theme: Breeze
Xcursor.size:  0

! Xterm
XTerm*background: #1f2126
XTerm*foreground: #5F6B85
XTerm*reverseVideo: on
XTerm*faceName: Terminus:size=11:antialias=true
XTerm*selectToClipboard: true

! XFT
Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.autohint:   false
Xft.hintstyle:  hintslight
Xft.lcdfilter:  lcddefault
Xft.rgba:       rgb


! ROFI
!rofi.dpi:                            168
rofi.dpi:                            96
rofi.color-enabled:                  true
rofi.color-window:                   argb:DD1f2126, argb:DD404452, #404452
rofi.color-normal:                   argb:00000000, #5F6B85, argb:00000000, #404452, #95A2BB
rofi.color-active:                   #1f2126, #61AEEE, #1f2126, #404452, #61AEEE
rofi.color-urgent:                   #1f2126, #E06C75, #1f2126, #404452, #E06C75
rofi.bw:                             0
rofi.width:                          50n
rofi.padding:                        300
rofi.fullscreen:                     true
rofi.fixed-num-lines:                true
rofi.lines:                          10
rofi.line-margin:                    8
rofi.separator-style:                solid
rofi.font:                           Droid Sans 18
rofi.eh:                             1


! URXVT
URxvt*font:                xft:Operator Mono Medium:style=Regular;size=12
URxvt*boldFont:            xft:Operator Mono Bold:style=Bold:size=12
URxvt*italicFont:          xft:Operator Mono Medium:style=Italic:size=12:italic
URxvt*boldItalicFont:      xft:Operator Mono Bold:style=Italic:size=12:italic
URxvt*letterSpace:         -1

URxvt*boldMode:                      on
URxvt*depth: 32

URxvt*background:                     #1F2126
URxvt*foreground:                     #abb2bf
URxvt*cursorColor:                    #abb2bf

URxvt*scrollColor:                    #dfe1e8
